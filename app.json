{
  "expo": {
    "name": "Versant",
    "slug": "versant-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/2x/VersantLogo@2x.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/2x/VersantSplashText@2x.png",
      "resizeMode": "contain",
      "backgroundColor": "#005E3A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
        [
          "@react-native-google-signin/google-signin",
          {
            "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
          }
        ]
    ]
  }
}
